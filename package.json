{
  "name": "npm-update-package",
  "version": "4.0.36",
  "description": "CLI tool for creating pull requests to update npm packages",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint --ignore-path .gitignore '**/*.{js,mjs,cjs,ts,mts,cts}'",
    "postbuild": "cp package.json dist/package.json",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm-run-all lint test build",
    "test": "glob -c 'node --test --import tsx/esm' 'src/**/*.test.ts'"
  },
  "bin": "dist/src/bin/npm-update-package.js",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@octokit/rest": "21.1.1",
    "commander": "12.1.0",
    "execa": "9.1.0",
    "fp-ts": "2.16.10",
    "http-status-codes": "2.3.0",
    "io-ts": "2.2.22",
    "jsdom": "21.1.2",
    "lodash": "4.17.21",
    "log4js": "6.9.1",
    "mustache": "4.2.0",
    "npm-check-updates": "16.3.11",
    "parse-github-url": "1.0.3",
    "safe-marked": "16.0.0",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@munierujp/eslint-config-typescript": "47.1.1",
    "@tsconfig/node20": "20.1.6",
    "@types/jsdom": "21.1.7",
    "@types/lodash": "4.17.18",
    "@types/mustache": "4.2.6",
    "@types/node": "20.14.12",
    "@types/parse-github-url": "1.0.3",
    "@types/semver": "7.5.8",
    "eslint": "8.57.0",
    "eslint-config-lodash": "4.0.3",
    "glob": "11.0.3",
    "nock": "14.0.5",
    "npm-run-all2": "8.0.4",
    "rimraf": "6.0.1",
    "test-each": "7.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "utility-types": "3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm-update-package/npm-update-package.git"
  },
  "bugs": {
    "url": "https://github.com/npm-update-package/npm-update-package/issues"
  },
  "homepage": "https://github.com/npm-update-package/npm-update-package",
  "license": "MIT",
  "keywords": [
    "dependencies",
    "dependency",
    "github enterprise",
    "github",
    "node",
    "npm",
    "outdated",
    "package",
    "update",
    "yarn"
  ]
}
